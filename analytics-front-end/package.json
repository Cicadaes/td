{
  "name": "aeplus-front-end",
  "version": "5.1.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 5000",
    "start-dev": "ng serve --port 5000",
    "build": "npm run lint && ng build --prod --aot",
    "build-prod": "npm run lint && ng build --prod --aot",
    "build-dev": "npm run lint && npm run lint-aot && ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint-aot": "ng build --prod",
    "e2e": "ng e2e",
    "commit-lint": "commitlint -E GIT_PARAMS",
    "format:fix": "pretty-quick --staged",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format:fixall": "pretty-quick --pattern 'src/**/*.*(js|html|less)'",
    "prod:test": "npm run build && http-server ./ -p 5000 --gzip --proxy http://172.23.4.168:50080"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "ajv": "^6.9.1",
    "core-js": "^2.5.4",
    "date-fns": "^1.30.1",
    "echarts": "^4.2.1",
    "echarts-config": "^0.1.4",
    "jointjs": "1.0.3",
    "jsencrypt": "^3.0.0-rc.1",
    "lodash": "4.17.11",
    "ng-zorro-antd": "1.8.1",
    "ng2-ckeditor": "1.2.1",
    "ngx-clipboard": "^11.1.9",
    "ngx-echarts": "^4.1.0",
    "perfect-scrollbar": "0.7.1",
    "rxjs": "6.3.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/cli": "~6.1.2",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/echarts": "^4.1.7",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "http-server": "^0.11.1",
    "husky": "^1.3.1",
    "less": "^2.7.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.9.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "run-s format:fix"
    }
  }
}
