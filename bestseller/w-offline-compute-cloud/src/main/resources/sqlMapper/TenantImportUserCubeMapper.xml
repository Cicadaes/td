<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="TenantImportUserCubeMapper">
    <select id="query" parameterType="map"
            resultType="com.talkingdata.wifianalytics.offline.compute.bean.TenantImportUserCube">
        SELECT * FROM tenant_import_user_cube WHERE
        <if test="tenant_id != null">
            tenant_id = #{tenant_id} AND
        </if>
        <if test="project_id != null">
            project_id = #{project_id} AND
        </if>
        <if test="crowd_id != null">
            crowd_id = #{crowd_id}
        </if>
        ORDER BY date desc limit 1
    </select>

</mapper>