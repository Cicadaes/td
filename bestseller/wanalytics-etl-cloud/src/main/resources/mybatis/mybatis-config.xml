        <mapper resource="mybatis/mapper/TenantHousingCoverageCountMapper.xml"/>
        <mapper resource="mybatis/mapper/OfflineCrossOldUserMonthMapper.xml"/>
        <mapper resource="mybatis/mapper/OfflineOlderUserMonthMapper.xml"/>
        <mapper resource="mybatis/mapper/TenantAppPromotionMapper.xml"/>
        <mapper resource="mybatis/mapper/TenantAppSignificanceMapper.xml"/>
        <mapper resource="mybatis/mapper/TenantUseAppRoutineMapper.xml"/>
        <mapper resource="mybatis/mapper/WifiPixTagCountMapper.xml"/>
        <mapper resource="mybatis/mapper/TargetManagementMapper.xml"/>
        <mapper resource="mybatis/mapper/TargetMapper.xml"/>
        <mapper resource="mybatis/mapper/CrowdBlackListMapper.xml"/>
        <mapper resource="mybatis/mapper/LookalikeCrowdMapper.xml"/>
        <mapper resource="mybatis/mapper/ProjectMapper.xml"/>
        <mapper resource="mybatis/mapper/ProjectParamMapper.xml"/>
