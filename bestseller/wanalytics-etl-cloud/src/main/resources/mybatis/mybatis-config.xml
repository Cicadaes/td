<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <settings>
        <!-- changes from the defaults for testing -->
        <setting name="cacheEnabled" value="false" />
        <setting name="useGeneratedKeys" value="true" />
        <setting name="defaultExecutorType" value="REUSE" />
    </settings>
    <environments default="development">
        <environment id="development">
            <transactionManager type="jdbc"/>
            <dataSource type="POOLED">
                <property name="driver" value="${wifianalyticsdb.connection.driver_class}"/>
                <property name="url" value="${wifianalyticsdb.connection.url}"/>
                <property name="username" value="${wifianalyticsdb.connection.username}"/>
                <property name="password" value="${wifianalyticsdb.connection.password}"/>
                <property name="poolMaximumActiveConnections" value="200" />
                <property name="poolMaximumIdleConnections" value="0" />
                <property name="poolMaximumCheckoutTime" value="200"/>
            </dataSource>
        </environment>
    </environments>
    <mappers>
        <mapper resource="mybatis/mapper/ProjectRelationMapper.xml"/>
        <mapper resource="mybatis/mapper/TenantStayDurationMapper.xml"/>
        <mapper resource="mybatis/mapper/TenantDeviceCountMapper.xml"/>
        <mapper resource="mybatis/mapper/TenantJobHousingCountMapper.xml"/>
        <mapper resource="mybatis/mapper/TenantRegionCountMapper.xml"/>
        <mapper resource="mybatis/mapper/TenantStayDurationMapper.xml"/>
