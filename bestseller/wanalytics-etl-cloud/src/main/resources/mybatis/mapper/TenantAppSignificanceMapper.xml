    <insert id="batchInsert" useGeneratedKeys="true" parameterType="java.util.List">
        <selectKey resultType="java.lang.Integer" order="AFTER" keyProperty="id">
            SELECT LAST_INSERT_ID()
        </selectKey>
        INSERT INTO
        TD_TENANT_APP_SIGNIFICANCE(tenant_id,project_id,crowd_id,run_date,platform,app_hash,app_proportion,city_app_proportion,significance_value,app_type,hour,cycle_statistics,start_date,end_date,type_id,type_name)
        VALUES
        <foreach collection="list" item="item" index="index" separator=",">
            (#{item.tenantId},#{item.projectId},#{item.crowdId},#{item.runDate},#{item.platform},#{item.appHash},#{item.appProportion},#{item.cityAppProportion},#{item.significanceValue},#{item.appType},#{item.hour},#{item.cycleStatistics},#{item.startDate},#{item.endDate},#{item.typeId},#{item.typeName})
        </foreach>
    </insert>

    <insert id="batchSelectAndInsert" useGeneratedKeys="true" parameterType="java.util.List">
        <selectKey resultType="java.lang.Integer" order="AFTER" keyProperty="id">
            SELECT LAST_INSERT_ID()
        </selectKey>
        INSERT INTO TD_TENANT_APP_SIGNIFICANCE(tenant_id, project_id, crowd_id, run_date, platform, app_hash,
        app_proportion, city_app_proportion, significance_value, app_type, hour, cycle_statistics, start_date, end_date,
        type_id, type_name)
        SELECT t.tenant_id, d.project_id, c.crowd_id, t.run_date, t.platform, t.app_hash, t.app_proportion,
        t.city_app_proportion, t.significance_value, t.app_type, t.hour, t.cycle_statistics, t.start_date, t.end_date,
        t.type_id, t.type_name
        FROM
        (SELECT s.tenant_id, s.type, s.run_date, s.platform, s.app_hash, s.app_type, s.hour, s.cycle_statistics,
        s.start_date, s.end_date, s.type_id, s.type_name, AVG(IFNULL(s.app_proportion,0)) AS app_proportion,
        AVG(IFNULL(s.city_app_proportion,0)) AS city_app_proportion, AVG(IFNULL(s.significance_value,0)) AS
        significance_value
        FROM
        (SELECT DISTINCT tb.tenant_id, cd.type, tb.run_date, tb.platform, tb.app_hash, tb.app_type, tb.hour,
        tb.cycle_statistics, tb.start_date, tb.end_date, tb.type_id, tb.type_name, tb.app_proportion,
        tb.city_app_proportion, tb.significance_value, tb.project_id
        FROM TD_TENANT_APP_SIGNIFICANCE tb
        JOIN TD_CROWD cd
        ON tb.project_id = cd.attr1
        AND tb.crowd_id = cd.id
        WHERE tb.project_id IN (
        SELECT r.project_id FROM TD_PROJECT_RELATION r
        WHERE r.project_parent_id = #{projectId})
        AND tb.run_date = #{runDate}
        )s
        GROUP BY s.tenant_id, s.type, s.run_date, s.platform, s.app_hash, s.app_type, s.hour, s.cycle_statistics,
        s.start_date, s.end_date, s.type_id, s.type_name
        )t,
        (SELECT DISTINCT project_parent_id AS project_id FROM TD_PROJECT_RELATION WHERE project_parent_id = #{projectId}
        )d,
        (SELECT DISTINCT id AS crowd_id, type, attr1 FROM TD_CROWD
        )c
        WHERE c.attr1 = d.project_id
        AND c.type = t.type
    </insert>
</mapper>
