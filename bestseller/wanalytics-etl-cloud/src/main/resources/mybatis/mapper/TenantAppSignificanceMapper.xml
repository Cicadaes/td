    <!-- queryAppByList -->
    <select id="queryAppByList" resultMap="AssociationMap" parameterType="map">
        select
        t.*,t1.app_name as appName,t1.app_icon as appIcon
        from TD_TENANT_APP_SIGNIFICANCE t
        inner join TD_APP_INFO t1
        ON t1.app_hash=t.app_hash and t1.platform = t.platform
        <where>
            1=1
            and t1.app_icon is not NULL
            and t1.app_name is not null
            <if test="projectId !=null ">
                and t.project_id=#{projectId}
            </if>
            <if test="tenantId !=null ">
                and t.tenant_id=#{tenantId}
            </if>
            <if test="crowdId !=null ">
                and t.crowd_id=#{crowdId}
            </if>
            <if test=" startDate != null and startDate != '' ">
                AND t.start_date = #{startDate}
            </if>
            <if test=" endDate != null and endDate != '' ">
                AND t.end_date = #{endDate}
            </if>
            <if test="appType !=null ">
                AND t.type_id IN
                <foreach collection="appType" open="(" separator="," close=")" item="item">
                    #{item}
                </foreach>
            </if>
            <!--
             <if test="appType ==null  or  appType=='null'"> 全部应用类型
                 AND t.type_id is not null
             </if>
             -->
            <if test=" cycleStatistics != null and cycleStatistics != '' ">
                AND t.cycle_statistics = #{cycleStatistics}
            </if>
            <if test=" hour != null and hour != '' ">
                and t.hour IN
                <foreach collection="hour" open="(" separator="," close=")" item="item">
                    #{item}
                </foreach>
            </if>
            group by t.app_hash
            order by t.significance_value desc
        </where>
        <if test="appListLimit !=null and appListLimit!=0">
            limit #{appListLimit}
        </if>
    </select>

