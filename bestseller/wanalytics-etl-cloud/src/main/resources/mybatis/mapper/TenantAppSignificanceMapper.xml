    <!-- 查询App清单数据列表 -->
    <select id="queryToSyncAppInfo" resultMap="BaseResultMap" parameterType="Object">
        SELECT temp1.app_hash,temp1.platform
        FROM
        (
        SELECT DISTINCT
        t.app_hash,
        t.platform
        FROM
        TD_TENANT_APP_SIGNIFICANCE t
        ) temp1
        WHERE
        (
        temp1.app_hash,
        temp1.platform
        ) not IN (
        SELECT
        app_hash,
        platform
        FROM
        TD_APP_INFO
        )
    </select>

    <select id="queryChildrenSum" resultMap="BaseResultMap" parameterType="Object">
        SELECT s.tenant_id, s.crowd_id, s.platform, s.app_hash, s.app_type, s.hour, s.cycle_statistics, s.start_date,
        s.end_date, s.type_id, s.type_name, AVG(IFNULL(s.app_proportion,0)) AS app_proportion,
        AVG(IFNULL(s.city_app_proportion,0)) AS city_app_proportion, AVG(IFNULL(s.significance_value,0)) AS
        significance_value
        FROM TD_TENANT_APP_SIGNIFICANCE s WHERE s.project_id IN (
        SELECT r.project_id FROM TD_PROJECT_RELATION r
        WHERE r.project_parent_id = #{projectId})
        AND s.run_date = #{runDate}
        GROUP BY s.tenant_id, s.crowd_id, s.platform, s.app_hash, s.app_type, s.hour, s.cycle_statistics, s.start_date,
        s.end_date, s.type_id, s.type_name
    </select>

