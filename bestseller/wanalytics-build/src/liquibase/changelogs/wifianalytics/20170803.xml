<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">


    <changeSet author="yunlong.wang" id="1501751335430-1">
        <createTable tableName="TD_RELEVANCY_ANALYSISS">
            <column autoIncrement="true" name="id" type="BIGINT" remarks="主键">
                <constraints primaryKey="true"/>
            </column>
            <column name="tenant_id" remarks="租户id" type="BIGINT"/>
            <column name="project_id" remarks="项目id" type="BIGINT" />
            <column name="analysiss_name" remarks="路径分析名称" type="varchar(64)" />
            <column name="analysiss_crowd_id" remarks="路径分析人群ID" type="bigint" />
            <column name="analysiss_crowd_name" remarks="路径分析人群名称" type="varchar(64)" />
            <column name="analysiss_project_ids" remarks="路径分析选中的项目ID,多个用逗号分割" type="varchar(128)" />
            <column name="status" remarks="状态,1表示执行中,2表示完成" type="int(10)" />
            <column name="create_by" remarks="创建者账号" type="varchar(128)" />
            <column name="creator" remarks="创建人" type="varchar(128)" />
            <column name="create_time" remarks="创建时间" type="TIMESTAMP" defaultValueComputed="CURRENT_TIMESTAMP">
                <constraints nullable="false"/>
            </column>
            <column name="update_by" remarks="修改者账号" type="varchar(128)" />
            <column name="updater" remarks="修改人" type="varchar(128)" />
            <column name="update_time" remarks="修改时间" type="TIMESTAMP" defaultValueComputed="CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>