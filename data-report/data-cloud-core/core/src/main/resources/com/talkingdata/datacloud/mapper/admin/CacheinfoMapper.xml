<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.talkingdata.datacloud.dao.admin.CacheinfoDao" >
  <!-- Result Map-->
  <resultMap id="BaseResultMap" type="com.talkingdata.datacloud.entity.admin.Cacheinfo" >
	<result column="id" property="id"/>
	<result column="cache_name" property="cacheName"/>
	<result column="max_entity" property="maxEntity"/>
	<result column="max_live_time" property="maxLiveTime"/>
	<result column="description" property="description"/>
	<result column="system_code" property="systemCode"/>
	<result column="create_by" property="createBy"/>
	<result column="creator" property="creator"/>
	<result column="update_by" property="updateBy"/>
	<result column="create_time" property="createTime"/>
	<result column="update_time" property="updateTime"/>
  </resultMap>
       
  <!-- TD_CACHEINFO table all fields -->
  <sql id="Base_Column_List" >
	 id,cache_name,max_entity,max_live_time,description,system_code,create_by,creator,update_by,create_time,update_time
  </sql>
   
  <!-- 查询条件 -->
  <sql id="Base_Where_Clause">
    where 1=1
    <trim suffixOverrides="," >
	      <if test="id != null and id != ''" >
        and id =  #{id}
	  </if>
	      <if test="cacheName != null and cacheName != ''" >
        and cache_name =  #{cacheName}
	  </if>
	      <if test="maxEntity != null and maxEntity != ''" >
        and max_entity =  #{maxEntity}
	  </if>
	      <if test="maxLiveTime != null and maxLiveTime != ''" >
        and max_live_time =  #{maxLiveTime}
	  </if>
	      <if test="description != null and description != ''" >
        and description =  #{description}
	  </if>
	      <if test="systemCode != null and systemCode != ''" >
        and system_code =  #{systemCode}
	  </if>
	      <if test="createBy != null and createBy != ''" >
        and create_by =  #{createBy}
	  </if>
	      <if test="creator != null and creator != ''" >
        and creator =  #{creator}
	  </if>
	      <if test="updateBy != null and updateBy != ''" >
        and update_by =  #{updateBy}
	  </if>
	      <if test="createTime != null and createTime != ''" >
        and create_time =  #{createTime}
	  </if>
	      <if test="updateTime != null and updateTime != ''" >
        and update_time =  #{updateTime}
	  </if>
    </trim>
  </sql>
  
  <!-- 高级查询条件 -->
  <sql id="Base_Where_Clause_Search">
    where 1=1
    <trim suffixOverrides="," >
	      <if test="id != null and id != ''" >
        and id =  #{id}
	  </if>
	      <if test="cacheName != null and cacheName != ''" >
        and cache_name like CONCAT('%',#{cacheName},'%')
	  </if>
	      <if test="maxEntity != null and maxEntity != ''" >
        and max_entity =  #{maxEntity}
	  </if>
	      <if test="maxLiveTime != null and maxLiveTime != ''" >
        and max_live_time =  #{maxLiveTime}
	  </if>
	      <if test="description != null and description != ''" >
        and description like CONCAT('%',#{description},'%')
	  </if>
	      <if test="systemCode != null and systemCode != ''" >
        and system_code =  #{systemCode}
	  </if>
	      <if test="createBy != null and createBy != ''" >
        and create_by =  #{createBy}
	  </if>
	      <if test="creator != null and creator != ''" >
        and creator =  #{creator}
	  </if>
	      <if test="updateBy != null and updateBy != ''" >
        and update_by =  #{updateBy}
	  </if>
	      <if test="createTime != null and createTime != ''" >
        and create_time =  #{createTime}
	  </if>
	      <if test="updateTime != null and updateTime != ''" >
        and update_time =  #{updateTime}
	  </if>
	  <if test="updateTimeStart != null and updateTimeStart != ''" >
        and update_time &gt;=  #{updateTimeStart}
	  </if>
	  <if test="updateTimeEnd != null and updateTimeEnd != ''" >
        and update_time &lt;=  #{updateTimeEnd}
	  </if>
	  <if test="q != null and q != ''" >
        and cache_name like CONCAT('%',#{q},'%')
	  </if>
    </trim>
  </sql>
   
  <!-- 插入记录 -->
  <insert id="insert" parameterType="Object" >
    <selectKey resultType="java.lang.Integer" order="AFTER" keyProperty="id">
	  SELECT LAST_INSERT_ID()
    </selectKey>
    insert into TD_CACHEINFO(id,cache_name,max_entity,max_live_time,description,system_code,create_by,creator,update_by,create_time,update_time)
 values(#{id},#{cacheName},#{maxEntity},#{maxLiveTime},#{description},#{systemCode},#{createBy},#{creator},#{updateBy},now(),now())
  </insert>

  <!-- 根据id，修改记录-->  
  <update id="updateByPrimaryKey" parameterType="Object" >
    update TD_CACHEINFO set cache_name=#{cacheName},max_entity=#{maxEntity},max_live_time=#{maxLiveTime},description=#{description},system_code=#{systemCode},create_by=#{createBy},creator=#{creator},update_by=#{updateBy},create_time=now(),update_time=now() where id=#{id}
  </update>
 
  <!-- 修改记录，只修改只不为空的字段 -->
  <update id="updateByPrimaryKeySelective" parameterType="Object" >
    update TD_CACHEINFO set 
	<trim  suffixOverrides="," >
	<if test="cacheName != null  ">
		cache_name=#{cacheName},
	</if>
	<if test="maxEntity != null  ">
		max_entity=#{maxEntity},
	</if>
	<if test="maxLiveTime != null  ">
		max_live_time=#{maxLiveTime},
	</if>
	<if test="description != null  ">
		description=#{description},
	</if>
	<if test="systemCode != null  ">
		system_code=#{systemCode},
	</if>
	<if test="createBy != null  ">
		create_by=#{createBy},
	</if>
	<if test="creator != null  ">
		creator=#{creator},
	</if>
	<if test="updateBy != null  ">
		update_by=#{updateBy},
	</if>
	<if test="createTime != null  ">
		create_time=#{createTime},
	</if>
	<if test="updateTime != null  ">
		update_time=#{updateTime},
	</if>
	</trim> where id=#{id}
  </update>
 
  <!-- 根据id查询 系统的缓存信息 -->
  <select id="selectByPrimaryKey"  resultMap="BaseResultMap" parameterType="Object">
    select <include refid="Base_Column_List" /> 
	 from TD_CACHEINFO where id = #{id}
  </select>
  
  <!-- 删除记录 -->
  <delete id="deleteByPrimaryKey" parameterType="Object">
    delete 	 from TD_CACHEINFO where id = #{id}
  </delete>

  <!-- 系统的缓存信息 列表总数-->
  <select id="queryByCount" resultType="java.lang.Integer"  parameterType="Object">
	select count(1) from TD_CACHEINFO 
	<include refid="Base_Where_Clause"/>
  </select>
  	
  <!-- 查询系统的缓存信息列表 -->
  <select id="queryByList" resultMap="BaseResultMap"  parameterType="Object">
	select 
	<include refid="Base_Column_List"/>
	from TD_CACHEINFO 
	<include refid="Base_Where_Clause"/>
	<if test="pager.orderCondition != null and pager.orderCondition != ''" >
       ${pager.orderCondition}
    </if>
    <if test="pager.mysqlQueryCondition != null and pager.mysqlQueryCondition != ''" >
       ${pager.mysqlQueryCondition}
    </if>
  </select>
  
   <!-- 高级搜索 系统的缓存信息 列表总数-->
  <select id="queryWithSearchByCount" resultType="java.lang.Integer"  parameterType="Object">
	select count(1) from TD_CACHEINFO 
	<include refid="Base_Where_Clause_Search"/>
  </select>
  	
  <!-- 高级搜索 查询系统的缓存信息列表 -->
  <select id="queryWithSearchByList" resultMap="BaseResultMap"  parameterType="Object">
	select 
	<include refid="Base_Column_List"/>
	from TD_CACHEINFO 
	<include refid="Base_Where_Clause_Search"/>
	<if test="pager.orderCondition != null and pager.orderCondition != ''" >
       ${pager.orderCondition}
    </if>
    <if test="pager.mysqlQueryCondition != null and pager.mysqlQueryCondition != ''" >
       ${pager.mysqlQueryCondition}
    </if>
  </select>
  
  <select id="queryByCacheinfo"  resultMap="BaseResultMap" parameterType="Object">
  	select <include refid="Base_Column_List" />  
  	from TD_CACHEINFO 
  	<include refid="Base_Where_Clause"/>
  </select>
</mapper>   
