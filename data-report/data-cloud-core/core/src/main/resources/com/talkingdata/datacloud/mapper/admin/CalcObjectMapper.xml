	  	<include refid="Base_Where_Clause"/>
	</select>
	
  <select id="queryWithSearchByCount" resultType="java.lang.Integer"  parameterType="Object">
	select count(1) from TD_CALC_OBJECT 
	<include refid="Base_Where_Clause_Search"/>
	<if test="codes != null and codes.length > 0">
		and code in
		<foreach collection="codes" open="(" separator="," close=")" item="item">
			#{item}
		</foreach>
	</if>
  </select>
  	
  <select id="queryWithSearchByList" resultMap="BaseResultMap"  parameterType="Object">
	select 
	<include refid="Base_Column_List"/>
	from TD_CALC_OBJECT 
	<include refid="Base_Where_Clause_Search"/>
	<if test="codes != null and codes.length > 0">
		and code in
		<foreach collection="codes" open="(" separator="," close=")" item="item">
			#{item}
		</foreach>
	</if>
	<if test="pager.orderCondition != null and pager.orderCondition != ''" >
       ${pager.orderCondition}
    </if>
    <if test="pager.mysqlQueryCondition != null and pager.mysqlQueryCondition != ''" >
       ${pager.mysqlQueryCondition}
    </if>
  </select>
  
  <select id="queryMaxByCode" resultMap="BaseResultMap"  parameterType="Object">
  SELECT t1.id,t1.name,t1.code,t1.type,t1.param,t1.attr1,t1.attr2,t1.attr3,t1.attr4,t1.attr5,t1.create_by,t1.creator,t1.update_by,t1.create_time,t1.update_time,t1.status,t1.calc_status,t1.data_app_id,t1.tenant_id
   FROM TD_CALC_OBJECT t1,
	(SELECT CODE, MAX(create_time) max_day FROM TD_CALC_OBJECT GROUP BY CODE) t2
	WHERE t1.code = t2.code AND t1.create_time = t2.max_day AND t1.code = #{code}
  		<if test="tenantId != null and tenantId != ''" >
        and t1.tenant_id =  #{tenantId}
	  </if>
  </select>
  
  <!-- @Deprecated -->
  <delete id="deleteByExcludesTenantIds" parameterType="Object">
    delete from TD_CALC_OBJECT where tenant_id not in
	<foreach collection="tenantIds" open="(" separator="," close=")" item="item">
		#{item}
	</foreach>
  </delete>
  
  <delete id="deleteByExcludesCreaters" parameterType="Object">
    delete from TD_CALC_OBJECT where create_by not in
	<foreach collection="creaters" open="(" separator="," close=")" item="item">
		#{item}
	</foreach>
	and code = 'crowdExport'
  </delete>
</mapper>
