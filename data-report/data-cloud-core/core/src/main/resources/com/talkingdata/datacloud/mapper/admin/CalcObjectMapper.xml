	</if>
	<if test="creator != null  ">
		creator=#{creator},
	</if>
	<if test="updateBy != null  ">
		update_by=#{updateBy},
	</if>
	<if test="createTime != null  ">
		create_time=#{createTime},
	</if>
	<if test="updateTime != null  ">
		update_time=#{updateTime},
	</if>
	<if test="status != null  ">
		status=#{status},
	</if>
	<if test="calcStatus != null  ">
		calc_status=#{calcStatus},
	</if>
	<if test="dataAppId != null  ">
		data_app_id=#{dataAppId},
	</if>
	<if test="tenantId != null  ">
		tenant_id=#{tenantId},
	</if>
	</trim> where id=#{id}
  </update>
 
  <!-- 根据id查询 计算对象 -->
  <select id="selectByPrimaryKey"  resultMap="BaseResultMap" parameterType="Object">
    select <include refid="Base_Column_List" /> 
	 from TD_CALC_OBJECT where id = #{id}
  </select>
  
  <!-- 删除记录 -->
  <delete id="deleteByPrimaryKey" parameterType="Object">
    delete 	 from TD_CALC_OBJECT where id = #{id}
  </delete>

  <!-- 计算对象 列表总数-->
  <select id="queryByCount" resultType="java.lang.Integer"  parameterType="Object">
	select count(1) from TD_CALC_OBJECT 
	<include refid="Base_Where_Clause"/>
  </select>
  	
  <!-- 查询计算对象列表 -->
  <select id="queryByList" resultMap="BaseResultMap"  parameterType="Object">
	select 
	<include refid="Base_Column_List"/>
	from TD_CALC_OBJECT 
	<include refid="Base_Where_Clause"/>
	<if test="pager.orderCondition != null and pager.orderCondition != ''" >
       ${pager.orderCondition}
    </if>
    <if test="pager.mysqlQueryCondition != null and pager.mysqlQueryCondition != ''" >
       ${pager.mysqlQueryCondition}
    </if>
  </select>
  
  <select id="queryExportCalcRecordByCount" resultType="java.lang.Integer"  parameterType="Object">
		select count(1)
		from TD_CALC_OBJECT
		<include refid="Base_Where_Clause"/>
		<if test="codes != null and codes.length > 0">
			and code in
			<foreach collection="codes" open="(" separator="," close=")" item="item">
				#{item}
			</foreach>
		</if>
   </select>
	
	<select id="queryExportCalcRecordByList" resultMap="BaseResultMap"  parameterType="Object">
		select
		<include refid="Base_Column_List" />
		from TD_CALC_OBJECT
		<include refid="Base_Where_Clause"/>
		<if test="codes != null and codes.length > 0">
