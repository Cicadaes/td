	  </if>
	      <if test="status != null and status != ''" >
        and status =  #{status}
	  </if>
	      <if test="calcStatus != null and calcStatus != ''" >
        and calc_status =  #{calcStatus}
	  </if>
	      <if test="dataAppId != null and dataAppId != ''" >
        and data_app_id =  #{dataAppId}
	  </if>
	      <if test="tenantId != null and tenantId != ''" >
        and tenant_id =  #{tenantId}
	  </if>
    </trim>
  </sql>
   
   <sql id="Base_Where_Clause_Search">
    where 1=1
    <trim suffixOverrides="," >
	      <if test="id != null and id != ''" >
        and id =  #{id}
	  </if>
	      <if test="name != null and name != ''" >
        and name like CONCAT('%',#{name},'%')
	  </if>
	      <if test="code != null and code != ''" >
        and code like CONCAT('%',#{code},'%')
	  </if>
	      <if test="type != null and type != ''" >
        and type =  #{type}
	  </if>
	      <if test="param != null and param != ''" >
        and param =  #{param}
	  </if>
	      <if test="attr1 != null and attr1 != ''" >
        and attr1 =  #{attr1}
	  </if>
	      <if test="attr2 != null and attr2 != ''" >
        and attr2 =  #{attr2}
	  </if>
	      <if test="attr3 != null and attr3 != ''" >
        and attr3 =  #{attr3}
	  </if>
	      <if test="attr4 != null and attr4 != ''" >
        and attr4 =  #{attr4}
	  </if>
	      <if test="attr5 != null and attr5 != ''" >
        and attr5 =  #{attr5}
	  </if>
	      <if test="createBy != null and createBy != ''" >
        and create_by like CONCAT('%',#{createBy},'%')
	  </if>
	      <if test="creator != null and creator != ''" >
        and creator like CONCAT('%',#{creator},'%')
	  </if>
	      <if test="updateBy != null and updateBy != ''" >
        and update_by =  #{updateBy}
	  </if>
	      <if test="createTime != null and createTime != ''" >
        and create_time =  #{createTime}
	  </if>
	      <if test="updateTime != null and updateTime != ''" >
        and update_time =  #{updateTime}
	  </if>
	      <if test="status != null and status != ''" >
        and status =  #{status}
	  </if>
	      <if test="calcStatus != null" >
        and calc_status =  #{calcStatus}
	  </if>
	      <if test="dataAppId != null and dataAppId != ''" >
        and data_app_id =  #{dataAppId}
	  </if>
	      <if test="tenantId != null and tenantId != ''" >
        and tenant_id =  #{tenantId}
	  </if>
	  <if test="createTimeStart != null and createTimeStart != ''" >
        and create_time &gt;=  #{createTimeStart}
	  </if>
	  <if test="createTimeEnd != null and createTimeEnd != ''" >
        and create_time &lt;=  #{createTimeEnd}
	  </if>
	  <if test="q != null and q != ''" >
        and (name like CONCAT('%',#{q},'%') or code like CONCAT('%',#{q},'%'))
	  </if>
    </trim>
  </sql>
  
  <!-- 插入记录 -->
  <insert id="insert" parameterType="Object" >
    <selectKey resultType="java.lang.Integer" order="AFTER" keyProperty="id">
	  SELECT LAST_INSERT_ID()
    </selectKey>
    insert into TD_CALC_OBJECT(id,name,code,type,param,attr1,attr2,attr3,attr4,attr5,create_by,creator,update_by,create_time,update_time,status,calc_status,data_app_id,tenant_id)
 values(#{id},#{name},#{code},#{type},#{param},#{attr1},#{attr2},#{attr3},#{attr4},#{attr5},#{createBy},#{creator},#{updateBy},now(),now(),#{status},#{calcStatus},#{dataAppId},#{tenantId})
  </insert>

  <!-- 根据id，修改记录-->  
  <update id="updateByPrimaryKey" parameterType="Object" >
    update TD_CALC_OBJECT set name=#{name},code=#{code},type=#{type},param=#{param},attr1=#{attr1},attr2=#{attr2},attr3=#{attr3},attr4=#{attr4},attr5=#{attr5},create_by=#{createBy},creator=#{creator},update_by=#{updateBy},create_time=now(),update_time=now(),status=#{status},calc_status=#{calcStatus},data_app_id=#{dataAppId},tenant_id=#{tenantId} where id=#{id}
  </update>
 
  <!-- 修改记录，只修改只不为空的字段 -->
  <update id="updateByPrimaryKeySelective" parameterType="Object" >
    update TD_CALC_OBJECT set 
	<trim  suffixOverrides="," >
