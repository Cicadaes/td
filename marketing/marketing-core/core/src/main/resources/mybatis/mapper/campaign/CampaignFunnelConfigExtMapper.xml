<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.talkingdata.marketing.core.dao.campaign.CampaignFunnelConfigDao" >

    <delete id="deleteByCreateTime" parameterType="string">
        delete from TD_MKT_CAMPAIGN_FUNNEL_CONFIG WHERE create_time > #{createTime}
    </delete>
    <select id="selectByPrimaryKeys" parameterType="List" resultMap="BaseResultMap">
        SELECT <include refid="Base_Column_List"/>
        FROM TD_MKT_CAMPAIGN_FUNNEL_CONFIG
        WHERE id IN
        <foreach collection="list" index="index" item="id" open="(" separator="," close=")">
            #{id}
        </foreach>
    </select>

    <select id="findByCampaignId" resultMap="BaseResultMap" parameterType="java.lang.Integer">
        select <include refid="Base_Column_List" />
        from TD_MKT_CAMPAIGN_FUNNEL_CONFIG
        where `campaign_id` = #{campaignId}
    </select>
</mapper>
