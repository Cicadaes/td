<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.talkingdata.marketing.core.dao.admin.EmailSendJobInputDao" >

  <select id="findValid" resultMap="BaseResultMap"  parameterType="Object">
    select
    <include refid="Base_Column_List"/>
    from TD_MKT_EMAIL_SEND_JOB_INPUT
    where status=1
  </select>

  <delete id="deleteByCreateTime" parameterType="string">
    delete from TD_MKT_EMAIL_SEND_JOB_INPUT WHERE create_time > #{createTime}
  </delete>

</mapper>
